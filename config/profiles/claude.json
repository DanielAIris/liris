{
    "name": "Claude",
    "interface": {
        "prompt_field": {
            "type": "textarea",
            "placeholder": "Message Claude...",
            "detection": {
                "method": "findContour",
                "color_range": {
                    "lower": [240, 240, 240],
                    "upper": [255, 255, 255]
                }
            }
        },
        "submit_button": {
            "type": "button",
            "detection": {
                "method": "findContour",
                "color_range": {
                    "lower": [0, 0, 150],
                    "upper": [100, 100, 255]
                }
            }
        },
        "response_area": {
            "type": "div",
            "detection": {
                "method": "findContour",
                "color_range": {
                    "lower": [245, 245, 245],
                    "upper": [255, 255, 255]
                }
            }
        },
        "new_chat_button": {
            "type": "button",
            "detection": {
                "method": "findContour",
                "color_range": {
                    "lower": [230, 230, 230],
                    "upper": [255, 255, 255]
                }
            }
        }
    },
    "limits": {
        "tokens_per_prompt": 8000,
        "prompts_per_day": 30,
        "reset_time": "00:00:00",
        "cooldown_period": 120
    },
    "error_detection": {
        "patterns": [
            "rate limited",
            "please try again later",
            "too many requests",
            "something went wrong"
        ]
    }
}